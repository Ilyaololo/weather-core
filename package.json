{
  "name": "core",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/weather-widgets/core.git"
  },
  "homepage": "https://gitlab.com/weather-widgets/core#readme",
  "scripts": {
    "cache:clean": "ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/typeorm cache:clear",
    "dev": "better-npm-run dev",
    "docker": "docker-compose -f docker-compose.yml up -d --build --no-deps",
    "migration:down": "yarn run cache:clean && ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/typeorm migration:revert",
    "migration:respawn": "yarn run migration:down && yarn run migration:up",
    "migration:up": "yarn run cache:clean && ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/typeorm migration:run",
    "schema:drop": "yarn run cache:clean && ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/typeorm schema:drop",
    "schema:respawn": "yarn run schema:drop && yarn run schema:sync && yarn run migration:up",
    "schema:sync": "yarn run cache:clean && ts-node -r tsconfig-paths/register -r dotenv/config ./node_modules/.bin/typeorm schema:sync",
    "start": "better-npm-run start",
    "start:debug": "better-npm-run start:debug"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start": {
      "command": "ts-node -r tsconfig-paths/register -r dotenv/config src/app.ts",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:debug": {
      "command": "node --inspect -r ts-node/register -r tsconfig-paths/register -r dotenv/config src/app.ts",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@nestjs/common": "^5.3.6",
    "@nestjs/core": "^5.3.6",
    "@nestjs/elasticsearch": "^0.1.2",
    "@nestjs/graphql": "^5.2.2",
    "@nestjs/jwt": "^0.1.3",
    "@nestjs/microservices": "^5.3.6",
    "@nestjs/passport": "^5.0.2",
    "@nestjs/typeorm": "^5.2.1",
    "@nestjs/websockets": "^5.3.1",
    "amqp": "^0.2.7",
    "apollo-server-express": "^2.0.6",
    "cache-manager": "^2.9.0",
    "cache-manager-redis-store": "^1.5.0",
    "class-transformer": "^0.1.9",
    "compression": "^1.7.3",
    "csurf": "^1.9.0",
    "dotenv": "^6.0.0",
    "express-rate-limit": "^3.1.1",
    "graphql": "^14.0.2",
    "graphql-tools": "^3.1.1",
    "helmet": "^3.13.0",
    "joi": "^13.6.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "multer": "^1.3.1",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "pm2": "^3.1.2",
    "qs": "^6.5.2",
    "rate-limit-redis": "^1.5.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.2",
    "toobusy-js": "^0.5.1",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/amqp": "^0.2.4",
    "@types/cache-manager": "^1.2.6",
    "@types/compression": "^0.0.36",
    "@types/csurf": "^1.9.35",
    "@types/elasticsearch": "^5.0.26",
    "@types/express": "^4.16.0",
    "@types/express-rate-limit": "^2.9.3",
    "@types/helmet": "^0.0.40",
    "@types/jest": "^23.3.2",
    "@types/joi": "^13.4.4",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.9.4",
    "@types/passport-jwt": "^3.0.1",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
