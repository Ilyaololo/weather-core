{
  "name": "core",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/weather-widgets/core.git"
  },
  "homepage": "https://gitlab.com/weather-widgets/core#readme",
  "scripts": {
    "cache:clean": "ts-node ./node_modules/.bin/typeorm cache:clear",
    "dev": "better-npm-run dev",
    "docker": "docker-compose -f docker-compose.yml up -d --build --no-deps",
    "migration:down": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "migration:up": "ts-node ./node_modules/.bin/typeorm migration:run",
    "schema:sync": "yarn run migration:down && ts-node ./node_modules/.bin/typeorm schema:sync && yarn run migration:up",
    "start": "better-npm-run start",
    "start:debug": "better-npm-run start:debug"
  },
  "betterScripts": {
    "start": {
      "command": "ts-node -r tsconfig-paths/register src/app.ts",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:debug": {
      "command": "node --inspect -r ts-node/register -r tsconfig-paths/register src/app.ts",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev": {
      "command": "nodemon",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@nestjs/common": "^5.3.5",
    "@nestjs/core": "^5.3.4",
    "@nestjs/elasticsearch": "^0.1.2",
    "@nestjs/graphql": "^5.1.2",
    "@nestjs/jwt": "^0.1.3",
    "@nestjs/microservices": "^5.3.5",
    "@nestjs/passport": "^5.0.1",
    "@nestjs/typeorm": "^5.2.0",
    "@nestjs/websockets": "^5.3.1",
    "amqp": "^0.2.7",
    "apollo-server-express": "^2.0.6",
    "graphql": "^14.0.0",
    "graphql-tools": "^3.1.1",
    "joi": "^13.6.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "pm2": "^3.0.4",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.2",
    "toobusy-js": "^0.5.1",
    "typeorm": "^0.2.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/amqp": "^0.2.4",
    "@types/elasticsearch": "^5.0.26",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/joi": "^13.4.4",
    "@types/node": "^10.9.4",
    "@types/passport-jwt": "^3.0.1",
    "@types/toobusy-js": "^0.5.0",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.11",
    "better-npm-run": "^0.1.1",
    "concurrently": "^4.0.1",
    "dotenv": "^6.0.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.4",
    "ts-loader": "^5.0.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-config": "^7.5.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "author": "i.lovenkov <ilyaololo96@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.9.4"
  }
}
